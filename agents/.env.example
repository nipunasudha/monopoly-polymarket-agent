# Trading mode: "dry_run" (default) or "live"
# dry_run: full analysis pipeline, no order execution, no CLOB/wallet initialization
# live: real order execution on Polymarket (requires valid wallet key with USDC)
TRADING_MODE="dry_run"

# Architecture mode: Use new TradingHub-based architecture
# Set to "true" to use parallel research with TradingHub (Phase 7+)
# Set to "false" to use legacy sequential architecture
USE_NEW_ARCHITECTURE=false

# Simulated USDC balance used for trade sizing in dry_run mode
SIMULATED_USDC_BALANCE=1000.0

POLYGON_WALLET_PRIVATE_KEY=""
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""
EXA_API_KEY=""
TAVILY_API_KEY=""
NEWSAPI_API_KEY=""
